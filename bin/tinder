#!/usr/bin/env python3
# This file is placed in the Public Domain.


"objects"


import os
import shutil
import sys
import time


sys.path.insert(0, os.getcwd())


from objz.command import Commands, Config, command, parse
from objz.methods import fmt, getpath, ident
from objz.objects import Object
from objz.persist import find, fntime, read, skel, types, write
from objz.runtime import Event


PATH = ".test"


"classes"


class Log:
 
    pass


class Todo:

    pass


examples = {
    "cmd": [""],
    "dne": ["", "test2"],
    "log": ["", "test"],
    "fnd": ["", "log"],
    "tdo": ["", "test2"]
}


def main():
    nr = 100
    if os.path.exists(PATH):
        shutil.rmtree(PATH)
    for _x in range(nr):
        for cmd, example in examples.items():
            for ex in example:
                txt = f"{cmd} {ex}"
                evt = Event()
                parse(evt, txt.strip())
                command(evt)


if __name__ == "__main__":
    main()
    